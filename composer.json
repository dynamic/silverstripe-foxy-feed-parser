{
    "name": "dynamic/silverstripe-foxy-feed-parser",
    "description": "An add-on module to parse the Foxy.io datafeed.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "require": {
        "dynamic/silverstripe-foxy": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "theseer/phpdox": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Foxy\\Parser\\": "src/",
            "Dynamic\\Foxy\\Parser\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
